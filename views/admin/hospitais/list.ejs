<%- include('../../partials/header'); -%>
<div class="container">
    <hr>
    <h2>Hospitais</h2>
    <a class="btn btn-success" href="/admin/hospitais/new">Criar um novo hospital</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID:</th>
                <th>Nome:</th>
                 <th>CEP:</th>
                <th>CNPJ:</th>
                <th>Endereço:</th>
                <th>Número:</th>
                <th>Complemento:</th>
                <th>Bairro:</th>
                 <th>Cidade:</th>
                 <th>UF:</th>
            </tr>
        </thead>
        <tbody>
            <% hospitais.forEach(hospital => { %>
                <tr>
                    <th><%= hospital.id %></th>
                    <th><%= hospital.nome %></th>
                    <th><%= hospital.cep %></th>
                    <th><%= hospital.cnpj %></th>
                    <th><%= hospital.endereco %></th>
                    <th><%= hospital.numero %></th>
                    <th><%= hospital.complemento %></th>
                    <th><%= hospital.bairro %></th>
                    <th><%= hospital.cidade %></th>
                    <th><%= hospital.uf %></th>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

